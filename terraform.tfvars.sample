# Add a DO access token
do_token = "_your_token_"

# Add a task_name tag
tag_task_name = "task_name:_your_task_name"

# Add a user_email tag
tag_admin_email = "user_email:_your_email_"

# Add a new SSH key for admin access from file
admin_ssh_key_name = "_your_ssh_key_name"
admin_ssh_key_path = "/path/to/ssh-key"

admin_ssh_privkey_path = "/path/to/ssh-privkey"
vps_user_password = "your_vps_pass"
vps_user_name = "your_vps_username"

# Add an AWS access tokens
aws_access_key = "your_access_key"
aws_secret_key = "your_secret_key"

# Add an AWS Route53 Zone and Record name
aws_route53_zone = "your.zo.ne"
aws_route53_record_name = "your_record.your.zone"

# Add DO droplet count
do_vps_count = number_of_droplets